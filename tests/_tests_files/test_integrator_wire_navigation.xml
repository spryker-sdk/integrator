<?xml version="1.0"?>
<config>
    <before-main>
        <label>Before main</label>
        <title>Before main</title>
        <icon>fa-archive</icon>
        <controller>index</controller>
        <action>index</action>
        <pages>
            <before-main-nested>
                <label>Before main - nested</label>
                <title>Before main - nested</title>
                <icon>fa-archive</icon>
                <controller>index</controller>
                <action>index</action>
                <visible>0</visible>
                <bundle>before-main-nested</bundle>
            </before-main-nested>
        </pages>
        <bundle>before-main</bundle>
    </before-main>
    <main>
        <label>Main</label>
        <title>Main</title>
        <icon>fa-archive</icon>
        <bundle>main</bundle>
        <controller>index</controller>
        <action>index</action>
    </main>
    <after-main>
        <label>After main</label>
        <title>After main</title>
        <icon>fa-archive</icon>
        <controller>index</controller>
        <action>index</action>
        <visible>0</visible>
        <bundle>after-main</bundle>
    </after-main>
    <new-latest>
        <label>New latest</label>
        <title>New latest</title>
        <icon>fa-archive</icon>
        <controller>index</controller>
        <action>index</action>
        <visible>0</visible>
        <bundle>after-main</bundle>
    </new-latest>
